def next_element(codeline,k,elements,end=False,stop=()):           #å¾codelineçš„ç¬¬kè™•é–‹å§‹å°‹æ‰¾elementsï¼Œæ²’æœ‰å‰‡è¿”å›-1
    c=0      #æ‹¬è™Ÿ
    n=len(codeline)
    elements=list(elements)
    while k<n:
        element=codeline[k]
        if c==0:
            if element in elements:
                return k
            elif element in stop:
                if end:return k
                return -1
        if c<0:
          #  print('å¼·åˆ¶è¿”å›')
            return k
        if element in ('(','[','{'):c+=1
        elif element in (')',']','}'):c-=1
        k+=1
    if end:return n
    return -1
