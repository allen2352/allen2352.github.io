    def remove_dir(filepath):
        for i in disk.listdir(filepath):
            road=f'{filepath}/{i}'
            if disk.isdir(road):
                remove_dir(road)
            elif disk.isfile(road):
                disk.remove(road)
        disk.rmdir(filepath)
    disk=user['disk']
