        self.code.add_newtab('REPL')
        if cmd=='':
            if len(self.REPL_box[1])>0:
                code='\n'.join(self.REPL_box[1])
              #  print('code:',code)
                self.load_code(code)
            self.REPL_box=[True,[]]
        else:
            codelines = parse_code(cmd)
