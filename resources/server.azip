      #  print('ç²å–Content:',self.content)
    def reload(self):                     #å°‡self.contentè½‰ç‚ºå¯ä½¿ç”¨list
        box = self.content.split('|')[1:]
        self.box=[]
        for i in range(len(box)):
            name, ptr = box[i].split('*')
            self.box.append([name, int(ptr)])
