        if self.access_rights:
            box=[]
            for name,ptr in self.box:
                box+=[f'|{name}*'.encode('utf-8')+format_file_size(ptr)]
            byte=b''.join(box)
            self.file.file_size=len(byte)
            self.file.write(byte,0)
            self.content=byte.decode('utf-8')
