def get_icon(filename):
    key = filename.split('.')[-1]
    if key in ['png', 'jpg', 'jpeg', 'bmp', 'tiff', 'tif', 'ppm', 'WebP', 'ico']:
        icon = 'picture'
    elif key in ['mp3', 'wav']:
        icon = 'music'
    elif key in ['mp4', 'avi', 'mov', 'wmv', 'mkv', 'asf', 'flv', 'gif']:
        icon = 'video'
    elif key in ['ttc', 'fon', 'ttf']:
        icon = 'font'
    elif key in ['docx', 'doc', 'ppt', 'pptx', 'xlsx', 'xls', 'lnk']:
        icon = {'docx': 'word', 'doc': 'word', 'ppt': 'power point', 'pptx': 'power point', 'xlsx': 'excel', 'xls': 'excel','lnk': 'shortcut'}[key]
    elif key in ['txt', 'exe', 'pdf', 'py', 'zip', 'xml', 'rpf', 'ini', 'dll', 'c', 'cpp', 'rar', 'js', 'html', 'dev','pyc', 'url', 'css']:
        icon = key
    else:
        icon = 'file'
    return icon
def index_change_path(user,request,folderpath=None):      #æ›´æ–°è·¯å¾‘æ¡†&listdiræ¡†
    road=request.GET_param['folderpath'] if folderpath==None else folderpath
    f_road = road.split('/')
    pathbox = []
    now_path = ''
    layer = 0
    for froad in f_road:  # ç´…è‰²æ¡†
        if froad != '' or layer == 0:
            now_path += froad + '/'
            pathbox.append(
                f'<input type="button" value="{froad}" style="border: 1px;background-color : #FFFFFF;" onclick="change_folder(\'{now_path}\');">>')
        layer += 1
    user['last_dir'] = now_path
    now_path_html=''.join(pathbox)
    if folderpath==None:
        http_response = Http_Response(200)
        http_response.content = now_path_html.encode('utf-8')
        return http_response
    return now_path_html
def index_folder_listdir(user,request,chosen_filename=''):      #æ›´æ–°è·¯å¾‘listdiræ¡†
    disk=user['disk']
    now_path=request.GET_param['folderpath'] if 'folderpath' in request.GET_param else user['last_dir']
    if now_path=='':               #ä¼ºæœå™¨è¦æ±‚åˆ·æ–°ç•¶å‰è³‡æ–™å¤¾è³‡è¨Š
        now_path=user['last_dir']
    chosen_file=request.GET_param['chosen_filename'] if 'chosen_filename' in request.GET_param else chosen_filename
    content_box = []
    if now_path[-1]!='/':now_path+='/'
