                # é€²è¡Œç™»è¨˜
                info.update(pinfo)
            else:
                else_codelines=[]
            for_loop=For_loop(vnames,base_obj,fp_codelines,else_codelines)
            # é€²è¡Œç™»è¨˜
            info.write(for_loop,(codeline[0][2],codeline[0][1]))
            row=q-1
        elif codeline[1]=='import':
            importer.need_load=1
            package_dict={}
            k,n=2,len(codeline)
