        else:
            equal=next_element(codeline,0,('=','+=','-=','*=','/=','%=','//=','**=','<<=','>>=','&=','^=','|='),stop=['lambda'])                             #è™•ç†=
            op=codeline[equal]      #operator
            variablebox=[]                          #æ”¶é›†[(var1,var2,...),(var1,var2,...),...]
            s=1
            while equal>0:
                vars=parse_codeline(codeline[s:equal],namespace)
                if type(vars) in (Tuple,List):
                    for var in vars:
                        if type(var)==Variable:
